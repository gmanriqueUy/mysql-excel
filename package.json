{
  "name": "mysql-excel",
  "version": "0.0.1",
  "description": "A CLI application to run queries on a mysql database and save the results in excel files",
  "engines": {
    "node": ">=8"
  },
  "main": "./lib/index.js",
  "bin": {
    "mysql-excel": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "build:local": "npm run build && npm i -g",
    "prepare": "npm run build"
  },
  "files": [
    "lib/",
    "README.md"
  ],
  "keywords": [
    "mysql",
    "excel",
    "export",
    "query"
  ],
  "preferGlobal": true,
  "author": "Gonzalo Manrique",
  "license": "MIT",
  "dependencies": {
    "commander": "^6.2.1",
    "inquirer": "^7.3.3",
    "mysql2": "^2.2.5",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.17",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
